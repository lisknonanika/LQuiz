<!DOCTYPE html>
<html>
<head>
    <meta content="width=device-width,initial-scale=1.0" name="viewport">
    <meta charset="utf-8">
    <title>LQuiz:Open Question Page</title>
    <%- include('parts/head') %>
    <script>
        const getOpenQuestion = async (offset) => {
            const ret = await getQuestion(true, offset);
            document.querySelector("#question-list").innerHTML = JSON.stringify(ret.response);
        }
    </script>
</head>
<body onload="getOpenQuestion(0)">
    <form method="post" id="frm"></form>
    <%- include('parts/menu', {current_menu: 1}) %>
    <div class="container">
        <div class="row">
            <div class="col-12" id="question-list"></div>
        </div>
    </div>
</body>
</html>